{
  "pli-tv-bi-vb-pc61:1.1": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc61:2.1": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc61:3.1": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc61:4.1": "Gabbhinivagga",
  "pli-tv-bi-vb-pc61:5.1": "61. Gabbhinisikkhāpada",
  "pli-tv-bi-vb-pc61:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc61:6.2": "Tena kho pana samayena bhikkhuniyo gabbhiniṃ vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc61:6.3": "Sā piṇḍāya carati.",
  "pli-tv-bi-vb-pc61:6.4": "Manussā evamāhaṃsu—",
  "pli-tv-bi-vb-pc61:6.5": "“dethāyyāya bhikkhaṃ, garubhārā ayyā”ti.",
  "pli-tv-bi-vb-pc61:7.1": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc61:7.2": "“kathañhi nāma bhikkhuniyo gabbhiniṃ vuṭṭhāpessantī”ti.",
  "pli-tv-bi-vb-pc61:8.1": "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ.",
  "pli-tv-bi-vb-pc61:8.2": "Yā tā bhikkhuniyo appicchā … pe …",
  "pli-tv-bi-vb-pc61:8.3": "tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc61:8.4": "“kathañhi nāma bhikkhuniyo gabbhiniṃ vuṭṭhāpessantī”ti … pe …",
  "pli-tv-bi-vb-pc61:8.5": "“saccaṃ kira, bhikkhave, bhikkhuniyo gabbhiniṃ vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc61:8.6": "“Saccaṃ, bhagavā”ti.",
  "pli-tv-bi-vb-pc61:8.7": "Vigarahi buddho bhagavā … pe …",
  "pli-tv-bi-vb-pc61:8.8": "kathañhi nāma, bhikkhave, bhikkhuniyo gabbhiniṃ vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc61:8.9": "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …",
  "pli-tv-bi-vb-pc61:8.10": "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—",
  "pli-tv-bi-vb-pc61:9.1": "“Yā pana bhikkhunī gabbhiniṃ vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc61:10.1": "Yā panāti",
  "pli-tv-bi-vb-pc61:10.2": "yā yādisā … pe …",
  "pli-tv-bi-vb-pc61:10.3": "bhikkhunīti",
  "pli-tv-bi-vb-pc61:10.4": "… pe …",
  "pli-tv-bi-vb-pc61:10.5": "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc61:11.1": "Gabbhinī nāma",
  "pli-tv-bi-vb-pc61:11.2": "āpannasattā vuccati.",
  "pli-tv-bi-vb-pc61:11.3": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc61:11.4": "upasampādeyya.",
  "pli-tv-bi-vb-pc61:12.1": "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:12.2": "Ñattiyā dukkaṭaṃ.",
  "pli-tv-bi-vb-pc61:12.3": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc61:12.4": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc61:12.5": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:13.1": "Gabbhiniyā gabbhinisaññā vuṭṭhāpeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc61:13.2": "Gabbhiniyā vematikā vuṭṭhāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:13.3": "Gabbhiniyā agabbhinisaññā vuṭṭhāpeti, anāpatti.",
  "pli-tv-bi-vb-pc61:13.4": "Agabbhiniyā gabbhinisaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:13.5": "Agabbhiniyā vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:13.6": "Agabbhiniyā agabbhinisaññā, anāpatti.",
  "pli-tv-bi-vb-pc61:14.1": "Anāpatti—",
  "pli-tv-bi-vb-pc61:14.2": "gabbhiniṃ agabbhinisaññā vuṭṭhāpeti, agabbhiniṃ agabbhinisaññā vuṭṭhāpeti, ummattikāya, ādikammikāyāti.",
  "pli-tv-bi-vb-pc61:15.1": "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
}