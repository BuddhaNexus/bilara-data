{
  "pli-tv-bi-vb-as1-7:1.1": "Theravāda Vinaya",
  "pli-tv-bi-vb-as1-7:2.1": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-as1-7:3.1": "Adhikaraṇasamatha",
  "pli-tv-bi-vb-as1-7:4.1": "Adhikaraṇasamathā:",
  "pli-tv-bi-vb-as1-7:4.2": "Tiṇavatthāraka",
  "pli-tv-bi-vb-as1-7:5.1": "Ime kho panāyyāyo satta adhikaraṇasamathā dhammā uddesaṃ āgacchanti.",
  "pli-tv-bi-vb-as1-7:6.1": "Uppannuppannānaṃ adhikaraṇānaṃ samathāya vūpasamāya sammukhāvinayo dātabbo, sativinayo dātabbo, amūḷhavinayo dātabbo, paṭiññāya kāretabbaṃ, yebhuyyasikā, tassapāpiyasikā, tiṇavatthārakoti.",
  "pli-tv-bi-vb-as1-7:7.1": "Uddiṭṭhā kho, ayyāyo, satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as1-7:7.2": "Tatthāyyāyo pucchāmi—",
  "pli-tv-bi-vb-as1-7:7.3": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:7.4": "Dutiyampi pucchāmi—",
  "pli-tv-bi-vb-as1-7:7.5": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:7.6": "Tatiyampi pucchāmi—",
  "pli-tv-bi-vb-as1-7:7.7": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:7.8": "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti.",
  "pli-tv-bi-vb-as1-7:8.1": "Adhikaraṇasamathā niṭṭhitā.",
  "pli-tv-bi-vb-as1-7:9.1": "Uddiṭṭhaṃ kho, ayyāyo, nidānaṃ.",
  "pli-tv-bi-vb-as1-7:9.2": "Uddiṭṭhā aṭṭha pārājikā dhammā.",
  "pli-tv-bi-vb-as1-7:9.3": "Uddiṭṭhā sattarasa saṃghādisesā dhammā.",
  "pli-tv-bi-vb-as1-7:9.4": "Uddiṭṭhā tiṃsa nissaggiyā pācittiyā dhammā.",
  "pli-tv-bi-vb-as1-7:9.5": "Uddiṭṭhā chasaṭṭhisatā pācittiyā dhammā.",
  "pli-tv-bi-vb-as1-7:9.6": "Uddiṭṭhā aṭṭha pāṭidesanīyā dhammā.",
  "pli-tv-bi-vb-as1-7:9.7": "Uddiṭṭhā sekhiyā dhammā.",
  "pli-tv-bi-vb-as1-7:9.8": "Uddiṭṭhā satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as1-7:9.9": "Ettakaṃ tassa bhagavato suttāgataṃ suttapariyāpannaṃ anvaddhamāsaṃ uddesaṃ āgacchati.",
  "pli-tv-bi-vb-as1-7:9.10": "Tattha sabbāheva samaggāhi sammodamānāhi avivadamānāhi sikkhitabbanti.",
  "pli-tv-bi-vb-as1-7:10.1": "Bhikkhunivibhaṅgo niṭṭhito.",
  "pli-tv-bi-vb-as1-7:11.1": "Pācittiyapāḷi niṭṭhitā."
}