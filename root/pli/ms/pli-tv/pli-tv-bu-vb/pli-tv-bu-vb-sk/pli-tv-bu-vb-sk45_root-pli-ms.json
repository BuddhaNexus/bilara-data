{
  "pli-tv-bu-vb-sk45:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk45:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk45:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk45:4.1": "Kabaḷavagga",
  "pli-tv-bu-vb-sk45:5.1": "45. Kabaḷāvacchedakasikkhāpada",
  "pli-tv-bu-vb-sk45:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk45:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū kabaḷāvacchedakaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk45:7.2": "“Na kabaḷāvacchedakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk45:8.1": "Na kabaḷāvacchedakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk45:8.2": "Yo anādariyaṃ paṭicca kabaḷāvacchedakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk45:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk45:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk45:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk45:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk45:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk45:9.6": "khajjake,",
  "pli-tv-bu-vb-sk45:9.7": "phalāphale,",
  "pli-tv-bu-vb-sk45:9.8": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk45:9.9": "āpadāsu,",
  "pli-tv-bu-vb-sk45:9.10": "ummattakassa,",
  "pli-tv-bu-vb-sk45:9.11": "ādikammikassāti.",
  "pli-tv-bu-vb-sk45:10.1": "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
}