{
  "pli-tv-bu-vb-sk10:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk10:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk10:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk10:4.1": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk10:5.1": "10. Dutiyaukkhittakasikkhāpada",
  "pli-tv-bu-vb-sk10:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk10:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū ukkhittakāya antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk10:7.1": "“Na ukkhittakāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk10:8.1": "Na ukkhittakāya antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk10:8.2": "Yo anādariyaṃ paṭicca ekato vā ubhato vā ukkhipitvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk10:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk10:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk10:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk10:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk10:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk10:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk10:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk10:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk10:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk10:10.1": "Dasamasikkhāpadaṃ niṭṭhitaṃ.",
  "pli-tv-bu-vb-sk10:10.2": "Parimaṇḍalavaggo paṭhamo."
}