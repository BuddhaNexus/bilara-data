{
  "pli-tv-bu-vb-sk18:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk18:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk18:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk18:4.1": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk18:5.1": "18. Dutiyabāhuppacālakasikkhāpada",
  "pli-tv-bu-vb-sk18:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk18:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū bāhuppacālakaṃ antaraghare nisīdanti bāhuṃ olambentā …",
  "pli-tv-bu-vb-sk18:7.2": "“Na bāhuppacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk18:8.1": "Na bāhuppacālakaṃ antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk18:8.2": "Bāhuṃ paggahetvā nisīditabbaṃ.",
  "pli-tv-bu-vb-sk18:8.3": "Yo anādariyaṃ paṭicca bāhuppacālakaṃ antaraghare nisīdati bāhuṃ olambento, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk18:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk18:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk18:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk18:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk18:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk18:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk18:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk18:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk18:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk18:10.1": "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
}