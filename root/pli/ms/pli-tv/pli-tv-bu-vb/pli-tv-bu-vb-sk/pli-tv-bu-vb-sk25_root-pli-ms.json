{
  "pli-tv-bu-vb-sk25:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk25:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk25:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk25:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk25:0.5": "25. Ukkuṭikasikkhāpada",
  "pli-tv-bu-vb-sk25:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk25:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū ukkuṭikāya antaraghare gacchanti …",
  "pli-tv-bu-vb-sk25:1.3": "“Na ukkuṭikāya antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk25:1.4": "Na ukkuṭikāya antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk25:1.5": "Yo anādariyaṃ paṭicca ukkuṭikāya antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk25:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk25:1.7": "asañcicca …pe…",
  "pli-tv-bu-vb-sk25:1.8": "",
  "pli-tv-bu-vb-sk25:1.9": "",
  "pli-tv-bu-vb-sk25:1.10": "",
  "pli-tv-bu-vb-sk25:1.11": "",
  "pli-tv-bu-vb-sk25:1.12": "",
  "pli-tv-bu-vb-sk25:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk25:1.14": "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
}
