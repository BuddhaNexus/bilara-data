{
  "pli-tv-bu-vb-sk12:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk12:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk12:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk12:4.1": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk12:5.1": "12. Dutiyaujjagghikasikkhāpada",
  "pli-tv-bu-vb-sk12:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk12:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahāhasitaṃ hasantā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk12:7.2": "“Na ujjagghikāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk12:8.1": "Na ujjagghikāya antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk12:8.2": "Yo anādariyaṃ paṭicca mahāhasitaṃ hasanto antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk12:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk12:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk12:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk12:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk12:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk12:9.6": "hasanīyasmiṃ vatthusmiṃ mihitamattaṃ karoti,",
  "pli-tv-bu-vb-sk12:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk12:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk12:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk12:10.1": "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
}