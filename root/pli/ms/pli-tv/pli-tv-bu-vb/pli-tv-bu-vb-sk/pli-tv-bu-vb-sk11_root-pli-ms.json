{
  "pli-tv-bu-vb-sk11:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk11:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk11:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk11:4.1": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk11:5.1": "11. Ujjagghikasikkhāpada",
  "pli-tv-bu-vb-sk11:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk11:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahāhasitaṃ hasantā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk11:7.2": "“Na ujjagghikāya antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk11:8.1": "Na ujjagghikāya antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk11:8.2": "Yo anādariyaṃ paṭicca mahāhasitaṃ hasanto antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk11:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk11:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk11:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk11:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk11:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk11:9.6": "hasanīyasmiṃ vatthusmiṃ mihitamattaṃ karoti,",
  "pli-tv-bu-vb-sk11:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk11:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk11:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk11:10.1": "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
}