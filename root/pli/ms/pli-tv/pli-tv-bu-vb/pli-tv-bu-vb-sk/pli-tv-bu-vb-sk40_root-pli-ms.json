{
  "pli-tv-bu-vb-sk40:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk40:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk40:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk40:4.1": "Sakkaccavagga",
  "pli-tv-bu-vb-sk40:5.1": "40. Ālopasikkhāpada",
  "pli-tv-bu-vb-sk40:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk40:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū dīghaṃ ālopaṃ karonti …",
  "pli-tv-bu-vb-sk40:7.2": "“Parimaṇḍalaṃ ālopaṃ karissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk40:8.1": "Parimaṇḍalo ālopo kātabbo.",
  "pli-tv-bu-vb-sk40:8.2": "Yo anādariyaṃ paṭicca dīghaṃ ālopaṃ karoti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk40:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk40:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk40:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk40:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk40:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk40:9.6": "khajjake,",
  "pli-tv-bu-vb-sk40:9.7": "phalāphale,",
  "pli-tv-bu-vb-sk40:9.8": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk40:9.9": "āpadāsu,",
  "pli-tv-bu-vb-sk40:9.10": "ummattakassa,",
  "pli-tv-bu-vb-sk40:9.11": "ādikammikassāti.",
  "pli-tv-bu-vb-sk40:10.1": "Dasamasikkhāpadaṃ niṭṭhitaṃ.",
  "pli-tv-bu-vb-sk40:10.2": "Sakkaccavaggo catuttho."
}