{
  "pli-tv-bu-vb-sk24:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk24:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk24:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk24:4.1": "Khambhakatavagga",
  "pli-tv-bu-vb-sk24:5.1": "24. Dutiyaoguṇṭhitasikkhāpada",
  "pli-tv-bu-vb-sk24:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk24:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū sasīsaṃ pārupitvā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk24:7.2": "“Na oguṇṭhito antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk24:8.1": "Na oguṇṭhitena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk24:8.2": "Yo anādariyaṃ paṭicca sasīsaṃ pārupitvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk24:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk24:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk24:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk24:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk24:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk24:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk24:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk24:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk24:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk24:10.1": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}