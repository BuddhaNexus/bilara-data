{
  "pli-tv-bu-vb-sk20:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk20:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk20:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk20:4.1": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk20:5.1": "20. Dutiyasīsappacālakasikkhāpada",
  "pli-tv-bu-vb-sk20:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk20:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṃ antaraghare nisīdanti sīsaṃ olambentā …",
  "pli-tv-bu-vb-sk20:7.2": "“Na sīsappacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk20:8.1": "Na sīsappacālakaṃ antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk20:8.2": "Sīsaṃ paggahetvā nisīditabbaṃ.",
  "pli-tv-bu-vb-sk20:8.3": "Yo anādariyaṃ paṭicca sīsappacālakaṃ antaraghare nisīdati sīsaṃ olambento, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk20:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk20:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk20:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk20:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk20:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk20:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk20:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk20:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk20:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk20:10.1": "Dasamasikkhāpadaṃ niṭṭhitaṃ.",
  "pli-tv-bu-vb-sk20:10.2": "Ujjagghikavaggo dutiyo."
}