{
  "pli-tv-bu-vb-sk26:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk26:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk26:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk26:4.1": "Khambhakatavagga",
  "pli-tv-bu-vb-sk26:5.1": "26. Pallatthikasikkhāpada",
  "pli-tv-bu-vb-sk26:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk26:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū pallatthikāya antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk26:7.2": "“Na pallatthikāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk26:8.1": "Na pallatthikāya antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk26:8.2": "Yo anādariyaṃ paṭicca hatthapallatthikāya vā dussapallatthikāya vā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk26:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk26:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk26:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk26:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk26:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk26:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk26:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk26:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk26:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk26:10.1": "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
}