{
  "pli-tv-bu-vb-sk33:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk33:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk33:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk33:4.1": "Sakkaccavagga",
  "pli-tv-bu-vb-sk33:5.1": "33. Sapadānasikkhāpada",
  "pli-tv-bu-vb-sk33:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk33:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṃ tahaṃ omasitvā piṇḍapātaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk33:7.2": "“Sapadānaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk33:8.1": "Sapadānaṃ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk33:8.2": "Yo anādariyaṃ paṭicca tahaṃ tahaṃ omasitvā piṇḍapātaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk33:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk33:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk33:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk33:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk33:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk33:9.6": "aññesaṃ dento omasati,",
  "pli-tv-bu-vb-sk33:9.7": "aññassa bhājane ākiranto omasati,",
  "pli-tv-bu-vb-sk33:9.8": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk33:9.9": "āpadāsu,",
  "pli-tv-bu-vb-sk33:9.10": "ummattakassa,",
  "pli-tv-bu-vb-sk33:9.11": "ādikammikassāti.",
  "pli-tv-bu-vb-sk33:10.1": "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
}