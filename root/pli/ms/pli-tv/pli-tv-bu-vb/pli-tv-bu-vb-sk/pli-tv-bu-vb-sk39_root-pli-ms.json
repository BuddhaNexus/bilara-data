{
  "pli-tv-bu-vb-sk39:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk39:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk39:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk39:4.1": "Sakkaccavagga",
  "pli-tv-bu-vb-sk39:5.1": "39. Kabaḷasikkhāpada",
  "pli-tv-bu-vb-sk39:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk39:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahantaṃ kabaḷaṃ karonti … pe …",
  "pli-tv-bu-vb-sk39:7.2": "“Nātimahantaṃ kabaḷaṃ karissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk39:8.1": "Nātimahanto kabaḷo kātabbo.",
  "pli-tv-bu-vb-sk39:8.2": "Yo anādariyaṃ paṭicca mahantaṃ kabaḷaṃ karoti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk39:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk39:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk39:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk39:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk39:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk39:9.6": "khajjake,",
  "pli-tv-bu-vb-sk39:9.7": "phalāphale,",
  "pli-tv-bu-vb-sk39:9.8": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk39:9.9": "āpadāsu,",
  "pli-tv-bu-vb-sk39:9.10": "ummattakassa,",
  "pli-tv-bu-vb-sk39:9.11": "ādikammikassāti.",
  "pli-tv-bu-vb-sk39:10.1": "Navamasikkhāpadaṃ niṭṭhitaṃ."
}