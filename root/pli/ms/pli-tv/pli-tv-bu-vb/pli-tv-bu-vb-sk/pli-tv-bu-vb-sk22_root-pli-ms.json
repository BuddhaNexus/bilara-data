{
  "pli-tv-bu-vb-sk22:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk22:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk22:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk22:4.1": "Khambhakatavagga",
  "pli-tv-bu-vb-sk22:5.1": "22. Dutiyakhambhakatasikkhāpada",
  "pli-tv-bu-vb-sk22:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk22:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū khambhakatā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk22:7.2": "“Na khambhakato antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk22:8.1": "Na khambhakatena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk22:8.2": "Yo anādariyaṃ paṭicca ekato vā ubhato vā khambhaṃ katvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk22:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk22:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk22:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk22:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk22:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk22:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk22:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk22:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk22:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk22:10.1": "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
}