{
  "pli-tv-bu-vb-sk47:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk47:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk47:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk47:4.1": "Kabaḷavagga",
  "pli-tv-bu-vb-sk47:5.1": "47. Hatthaniddhunakasikkhāpada",
  "pli-tv-bu-vb-sk47:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk47:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū hatthaniddhunakaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk47:7.2": "“Na hatthaniddhunakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk47:8.1": "Na hatthaniddhunakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk47:8.2": "Yo anādariyaṃ paṭicca hatthaniddhunakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk47:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk47:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk47:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk47:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk47:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk47:9.6": "kacavaraṃ chaḍḍento hatthaṃ niddhunāti,",
  "pli-tv-bu-vb-sk47:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk47:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk47:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk47:10.1": "Sattamasikkhāpadaṃ niṭṭhitaṃ."
}