{
  "pli-tv-bu-vb-sk4:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk4:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk4:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk4:4.1": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk4:5.1": "4. Dutiyasuppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk4:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk4:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṃ vivaritvā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk4:7.2": "“Suppaṭicchanno antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk4:8.1": "Suppaṭicchannena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk4:8.2": "Yo anādariyaṃ paṭicca kāyaṃ vivaritvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk4:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk4:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk4:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk4:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk4:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk4:9.6": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk4:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk4:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk4:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk4:10.1": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}