{
  "pli-tv-bu-vb-sk46:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk46:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk46:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk46:4.1": "Kabaḷavagga",
  "pli-tv-bu-vb-sk46:5.1": "46. Avagaṇḍakārakasikkhāpada",
  "pli-tv-bu-vb-sk46:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk46:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū avagaṇḍakārakaṃ bhuñjanti … pe …",
  "pli-tv-bu-vb-sk46:7.2": "“Na avagaṇḍakārakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk46:8.1": "Na avagaṇḍakārakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk46:8.2": "Yo anādariyaṃ paṭicca ekato vā ubhato vā gaṇḍaṃ katvā bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk46:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk46:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk46:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk46:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk46:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk46:9.6": "phalāphale,",
  "pli-tv-bu-vb-sk46:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk46:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk46:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk46:10.1": "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
}