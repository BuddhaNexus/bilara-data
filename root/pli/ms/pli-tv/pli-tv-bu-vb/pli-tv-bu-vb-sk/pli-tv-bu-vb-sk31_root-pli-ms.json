{
  "pli-tv-bu-vb-sk31:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk31:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk31:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk31:4.1": "Sakkaccavagga",
  "pli-tv-bu-vb-sk31:5.1": "31. Sakkaccasikkhāpada",
  "pli-tv-bu-vb-sk31:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk31:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū asakkaccaṃ piṇḍapātaṃ bhuñjanti abhuñjitukāmā viya …",
  "pli-tv-bu-vb-sk31:7.2": "“Sakkaccaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk31:8.1": "Sakkaccaṃ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk31:8.2": "Yo anādariyaṃ paṭicca asakkaccaṃ piṇḍapātaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk31:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk31:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk31:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk31:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk31:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk31:9.6": "āpadāsu,",
  "pli-tv-bu-vb-sk31:9.7": "ummattakassa,",
  "pli-tv-bu-vb-sk31:9.8": "ādikammikassāti.",
  "pli-tv-bu-vb-sk31:10.1": "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
}