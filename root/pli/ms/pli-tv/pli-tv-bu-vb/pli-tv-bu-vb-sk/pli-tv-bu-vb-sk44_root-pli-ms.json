{
  "pli-tv-bu-vb-sk44:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk44:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk44:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk44:4.1": "Kabaḷavagga",
  "pli-tv-bu-vb-sk44:5.1": "44. Piṇḍukkhepakasikkhāpada",
  "pli-tv-bu-vb-sk44:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk44:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍukkhepakaṃ bhuñjanti … pe …",
  "pli-tv-bu-vb-sk44:7.2": "“Na piṇḍukkhepakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk44:8.1": "Na piṇḍukkhepakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk44:8.2": "Yo anādariyaṃ paṭicca piṇḍukkhepakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk44:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk44:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk44:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk44:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk44:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk44:9.6": "khajjake,",
  "pli-tv-bu-vb-sk44:9.7": "phalāphale,",
  "pli-tv-bu-vb-sk44:9.8": "āpadāsu,",
  "pli-tv-bu-vb-sk44:9.9": "ummattakassa,",
  "pli-tv-bu-vb-sk44:9.10": "ādikammikassāti.",
  "pli-tv-bu-vb-sk44:10.1": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}