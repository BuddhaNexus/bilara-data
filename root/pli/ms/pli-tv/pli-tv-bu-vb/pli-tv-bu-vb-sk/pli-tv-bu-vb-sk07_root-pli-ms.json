{
  "pli-tv-bu-vb-sk7:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk7:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk7:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk7:4.1": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk7:5.1": "7. Okkhittacakkhusikkhāpada",
  "pli-tv-bu-vb-sk7:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk7:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṃ tahaṃ olokentā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk7:7.2": "“Okkhittacakkhu antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk7:8.1": "Okkhittacakkhunā antaraghare gantabbaṃ yugamattaṃ pekkhantena.",
  "pli-tv-bu-vb-sk7:8.2": "Yo anādariyaṃ paṭicca tahaṃ tahaṃ olokento antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk7:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk7:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk7:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk7:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk7:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk7:9.6": "āpadāsu,",
  "pli-tv-bu-vb-sk7:9.7": "ummattakassa,",
  "pli-tv-bu-vb-sk7:9.8": "ādikammikassāti.",
  "pli-tv-bu-vb-sk7:10.1": "Sattamasikkhāpadaṃ niṭṭhitaṃ."
}