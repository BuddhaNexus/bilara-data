{
  "pli-tv-bu-vb-sk29:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk29:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk29:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk29:4.1": "Khambhakatavagga",
  "pli-tv-bu-vb-sk29:5.1": "29. Samasūpakapaṭiggahaṇasikkhāpada",
  "pli-tv-bu-vb-sk29:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk29:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍapātaṃ paṭiggaṇhantā sūpaññeva bahuṃ paṭiggaṇhanti …",
  "pli-tv-bu-vb-sk29:8.1": "Sūpo nāma",
  "pli-tv-bu-vb-sk29:8.2": "dve sūpā— muggasūpo, māsasūpo hatthahāriyo.",
  "pli-tv-bu-vb-sk29:8.3": "Samasūpako piṇḍapāto paṭiggahetabbo.",
  "pli-tv-bu-vb-sk29:8.4": "Yo anādariyaṃ paṭicca sūpaññeva bahuṃ paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk29:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk29:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk29:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk29:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk29:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk29:9.6": "rasarase,",
  "pli-tv-bu-vb-sk29:9.7": "ñātakānaṃ,",
  "pli-tv-bu-vb-sk29:9.8": "pavāritānaṃ,",
  "pli-tv-bu-vb-sk29:9.9": "aññassatthāya,",
  "pli-tv-bu-vb-sk29:9.10": "attano dhanena,",
  "pli-tv-bu-vb-sk29:9.11": "āpadāsu,",
  "pli-tv-bu-vb-sk29:9.12": "ummattakassa,",
  "pli-tv-bu-vb-sk29:9.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk29:10.1": "Navamasikkhāpadaṃ niṭṭhitaṃ."
}