{
  "pli-tv-bu-vb-sk48:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk48:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk48:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk48:4.1": "Kabaḷavagga",
  "pli-tv-bu-vb-sk48:5.1": "48. Sitthāvakārakasikkhāpada",
  "pli-tv-bu-vb-sk48:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk48:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū sitthāvakārakaṃ bhuñjanti … pe …",
  "pli-tv-bu-vb-sk48:7.2": "“Na sitthāvakārakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk48:8.1": "Na sitthāvakārakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk48:8.2": "Yo anādariyaṃ paṭicca sitthāvakārakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk48:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk48:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk48:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk48:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk48:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk48:9.6": "kacavaraṃ chaḍḍento sitthaṃ chaḍḍayati,",
  "pli-tv-bu-vb-sk48:9.7": "āpadāsu,",
  "pli-tv-bu-vb-sk48:9.8": "ummattakassa,",
  "pli-tv-bu-vb-sk48:9.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk48:10.1": "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
}