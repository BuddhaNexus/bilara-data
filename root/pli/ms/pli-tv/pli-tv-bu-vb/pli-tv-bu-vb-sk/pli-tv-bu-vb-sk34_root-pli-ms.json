{
  "pli-tv-bu-vb-sk34:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk34:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk34:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk34:4.1": "Sakkaccavagga",
  "pli-tv-bu-vb-sk34:5.1": "34. Samasūpakasikkhāpada",
  "pli-tv-bu-vb-sk34:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk34:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍapātaṃ bhuñjantā sūpaññeva bahuṃ bhuñjanti …",
  "pli-tv-bu-vb-sk34:7.2": "“Samasūpakaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk34:8.1": "Sūpo nāma",
  "pli-tv-bu-vb-sk34:8.2": "dve sūpā—",
  "pli-tv-bu-vb-sk34:8.3": "muggasūpo, māsasūpo hatthahāriyo.",
  "pli-tv-bu-vb-sk34:8.4": "Samasūpako piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk34:8.5": "Yo anādariyaṃ paṭicca sūpaññeva bahuṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk34:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk34:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk34:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk34:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk34:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk34:9.6": "rasarase,",
  "pli-tv-bu-vb-sk34:9.7": "ñātakānaṃ,",
  "pli-tv-bu-vb-sk34:9.8": "pavāritānaṃ,",
  "pli-tv-bu-vb-sk34:9.9": "attano dhanena,",
  "pli-tv-bu-vb-sk34:9.10": "āpadāsu,",
  "pli-tv-bu-vb-sk34:9.11": "ummattakassa,",
  "pli-tv-bu-vb-sk34:9.12": "ādikammikassāti.",
  "pli-tv-bu-vb-sk34:10.1": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}