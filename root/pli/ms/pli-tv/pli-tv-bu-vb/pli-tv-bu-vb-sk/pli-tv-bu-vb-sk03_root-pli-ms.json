{
  "pli-tv-bu-vb-sk3:1.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk3:2.1": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk3:3.1": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk3:4.1": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk3:5.1": "3. Suppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk3:6.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk3:6.2": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṃ vivaritvā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk3:7.2": "“Suppaṭicchanno antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk3:8.1": "Suppaṭicchannena antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk3:8.2": "Yo anādariyaṃ paṭicca kāyaṃ vivaritvā antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk3:9.1": "Anāpatti—",
  "pli-tv-bu-vb-sk3:9.2": "asañcicca,",
  "pli-tv-bu-vb-sk3:9.3": "assatiyā,",
  "pli-tv-bu-vb-sk3:9.4": "ajānantassa,",
  "pli-tv-bu-vb-sk3:9.5": "gilānassa,",
  "pli-tv-bu-vb-sk3:9.6": "āpadāsu,",
  "pli-tv-bu-vb-sk3:9.7": "ummattakassa,",
  "pli-tv-bu-vb-sk3:9.8": "ādikammikassāti.",
  "pli-tv-bu-vb-sk3:10.1": "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
}