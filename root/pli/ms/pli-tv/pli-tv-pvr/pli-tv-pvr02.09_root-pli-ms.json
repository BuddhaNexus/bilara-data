{
  "pli-tv-pvr2.9:1.1": "Parivāra",
  "pli-tv-pvr2.9:2.1": "Bhikkhunivibhaṅga",
  "pli-tv-pvr2.9:3.1": "Dutiyabhāga",
  "pli-tv-pvr2.9:4.1": "Katthapaññattivāra",
  "pli-tv-pvr2.9:5.1": "1. Pārājikakaṇḍa",
  "pli-tv-pvr2.9:6.1": "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattaṃ?",
  "pli-tv-pvr2.9:6.2": "Kaṃ ārabbha?",
  "pli-tv-pvr2.9:6.3": "Kismiṃ vatthusmiṃ … pe …",
  "pli-tv-pvr2.9:6.4": "kenābhatanti?",
  "pli-tv-pvr2.9:7.1": "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-pvr2.9:7.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-pvr2.9:7.3": "Kaṃ ārabbhāti?",
  "pli-tv-pvr2.9:7.4": "Sundarīnandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-pvr2.9:7.5": "Kismiṃ vatthusminti?",
  "pli-tv-pvr2.9:7.6": "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyi, tasmiṃ vatthusmiṃ.",
  "pli-tv-pvr2.9:7.7": "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?",
  "pli-tv-pvr2.9:7.8": "Ekā paññatti.",
  "pli-tv-pvr2.9:7.9": "Anupaññatti anuppannapaññatti tasmiṃ natthi.",
  "pli-tv-pvr2.9:7.10": "Sabbatthapaññatti, padesapaññattīti?",
  "pli-tv-pvr2.9:7.11": "Sabbatthapaññatti.",
  "pli-tv-pvr2.9:7.12": "Sādhāraṇapaññatti, asādhāraṇapaññattīti?",
  "pli-tv-pvr2.9:7.13": "Asādhāraṇapaññatti.",
  "pli-tv-pvr2.9:7.14": "Ekatopaññatti, ubhatopaññattīti?",
  "pli-tv-pvr2.9:7.15": "Ekatopaññatti.",
  "pli-tv-pvr2.9:7.16": "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?",
  "pli-tv-pvr2.9:7.17": "Nidānogadhaṃ nidānapariyāpannaṃ?",
  "pli-tv-pvr2.9:7.18": "Katamena uddesena uddesaṃ āgacchatīti?",
  "pli-tv-pvr2.9:7.19": "Dutiyena uddesena uddesaṃ āgacchati.",
  "pli-tv-pvr2.9:7.20": "Catunnaṃ vipattīnaṃ katamā vipattīti?",
  "pli-tv-pvr2.9:7.21": "Sīlavipatti.",
  "pli-tv-pvr2.9:7.22": "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?",
  "pli-tv-pvr2.9:7.23": "Pārājikāpattikkhandho.",
  "pli-tv-pvr2.9:7.24": "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?",
  "pli-tv-pvr2.9:7.25": "Ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:7.26": "kāyato ca cittato ca samuṭṭhāti, na vācato … pe …",
  "pli-tv-pvr2.9:7.27": "kenābhatanti?",
  "pli-tv-pvr2.9:7.28": "Paramparābhataṃ—",
  "pli-tv-pvr2.9:8.1": "Upāli dāsako ceva,",
  "pli-tv-pvr2.9:8.2": "soṇako siggavo tathā;",
  "pli-tv-pvr2.9:8.3": "Moggaliputtena pañcamā,",
  "pli-tv-pvr2.9:8.4": "ete jambusirivhaye.",
  "pli-tv-pvr2.9:9.1": "… pe …",
  "pli-tv-pvr2.9:10.1": "Ete nāgā mahāpaññā,",
  "pli-tv-pvr2.9:10.2": "Vinayaññū maggakovidā;",
  "pli-tv-pvr2.9:10.3": "Vinayaṃ dīpe pakāsesuṃ,",
  "pli-tv-pvr2.9:10.4": "Piṭakaṃ tambapaṇṇiyāti. #1",
  "pli-tv-pvr2.9:11.1": "Vajjappaṭicchādanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-pvr2.9:11.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-pvr2.9:11.3": "Kaṃ ārabbhāti?",
  "pli-tv-pvr2.9:11.4": "Thullanandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-pvr2.9:11.5": "Kismiṃ vatthusminti?",
  "pli-tv-pvr2.9:11.6": "Thullanandā bhikkhunī jānaṃ pārājikaṃ dhammaṃ ajjhāpannaṃ bhikkhuniṃ nevattanā paṭicodesi na gaṇassa ārocesi, tasmiṃ vatthusmiṃ.",
  "pli-tv-pvr2.9:11.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:11.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:11.9": "dhuranikkhepe … pe …. #2",
  "pli-tv-pvr2.9:12.1": "Yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-pvr2.9:12.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-pvr2.9:12.3": "Kaṃ ārabbhāti?",
  "pli-tv-pvr2.9:12.4": "Thullanandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-pvr2.9:12.5": "Kismiṃ vatthusminti?",
  "pli-tv-pvr2.9:12.6": "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ anuvatti, tasmiṃ vatthusmiṃ.",
  "pli-tv-pvr2.9:12.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:12.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:12.9": "dhuranikkhepe … pe …. #3",
  "pli-tv-pvr2.9:13.1": "Aṭṭhamaṃ vatthuṃ paripūraṇapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-pvr2.9:13.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-pvr2.9:13.3": "Kaṃ ārabbhāti?",
  "pli-tv-pvr2.9:13.4": "Chabbaggiyā bhikkhuniyo ārabbha.",
  "pli-tv-pvr2.9:13.5": "Kismiṃ vatthusminti?",
  "pli-tv-pvr2.9:13.6": "Chabbaggiyā bhikkhuniyo aṭṭhamaṃ vatthuṃ paripūresuṃ, tasmiṃ vatthusmiṃ.",
  "pli-tv-pvr2.9:13.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:13.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:13.9": "dhuranikkhepe … pe …. #4",
  "pli-tv-pvr2.9:14.1": "Pārājikā niṭṭhitā."
}