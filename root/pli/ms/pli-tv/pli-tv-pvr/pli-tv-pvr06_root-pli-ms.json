{
  "pli-tv-pvr6:1.1": "Parivāra",
  "pli-tv-pvr6:2.1": "6. Khandhakapucchāvāra",
  "pli-tv-pvr6:3.1": "Upasampadaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:3.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:3.3": "Upasampadaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:3.4": "Samukkaṭṭhapadānaṃ dve āpattiyo. #1",
  "pli-tv-pvr6:4.1": "Uposathaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:4.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:4.3": "Uposathaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:4.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #2",
  "pli-tv-pvr6:5.1": "Vassūpanāyikaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:5.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:5.3": "Vassūpanāyikaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:5.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #3",
  "pli-tv-pvr6:6.1": "Pavāraṇaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:6.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:6.3": "Pavāraṇaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:6.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #4",
  "pli-tv-pvr6:7.1": "Cammasaññuttaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:7.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:7.3": "Cammasaññuttaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:7.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #5",
  "pli-tv-pvr6:8.1": "Bhesajjaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:8.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:8.3": "Bhesajjaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:8.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #6",
  "pli-tv-pvr6:9.1": "Kathinakaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:9.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:9.3": "Kathinakaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:9.4": "Samukkaṭṭhapadānaṃ natthi tattha āpatti. #7",
  "pli-tv-pvr6:10.1": "Cīvarasaññuttaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:10.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:10.3": "Cīvarasaññuttaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:10.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #8",
  "pli-tv-pvr6:11.1": "Campeyyakaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:11.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:11.3": "Campeyyakaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:11.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #9",
  "pli-tv-pvr6:12.1": "Kosambakaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:12.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:12.3": "Kosambakaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:12.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #10",
  "pli-tv-pvr6:13.1": "Kammakkhandhakaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:13.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:13.3": "Kammakkhandhakaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:13.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #11",
  "pli-tv-pvr6:14.1": "Pārivāsikaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:14.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:14.3": "Pārivāsikaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:14.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #12",
  "pli-tv-pvr6:15.1": "Samuccayaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:15.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:15.3": "Samuccayaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:15.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #13",
  "pli-tv-pvr6:16.1": "Samathaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:16.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:16.3": "Samathaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:16.4": "Samukkaṭṭhapadānaṃ dve āpattiyo. #14",
  "pli-tv-pvr6:17.1": "Khuddakavatthukaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:17.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:17.3": "Khuddakavatthukaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:17.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #15",
  "pli-tv-pvr6:18.1": "Senāsanaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:18.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:18.3": "Senāsanaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:18.4": "Samukkaṭṭhapadānaṃ tisso āpattiyo. #16",
  "pli-tv-pvr6:19.1": "Saṃghabhedaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:19.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:19.3": "Saṃghabhedaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:19.4": "Samukkaṭṭhapadānaṃ dve āpattiyo. #17",
  "pli-tv-pvr6:20.1": "Samācāraṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:20.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:20.3": "Samācāraṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:20.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #18",
  "pli-tv-pvr6:21.1": "Ṭhapanaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:21.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:21.3": "Ṭhapanaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:21.4": "Samukkaṭṭhapadānaṃ ekā āpatti. #19",
  "pli-tv-pvr6:22.1": "Bhikkhunikkhandhakaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:22.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:22.3": "Bhikkhunikkhandhakaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:22.4": "Samukkaṭṭhapadānaṃ dve āpattiyo. #20",
  "pli-tv-pvr6:23.1": "Pañcasatikaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:23.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:23.3": "Pañcasatikaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:23.4": "Samukkaṭṭhapadānaṃ natthi tattha āpatti. #21",
  "pli-tv-pvr6:24.1": "Sattasatikaṃ pucchissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:24.2": "Samukkaṭṭhapadānaṃ kati āpattiyo;",
  "pli-tv-pvr6:24.3": "Sattasatikaṃ vissajjissaṃ sanidānaṃ saniddesaṃ,",
  "pli-tv-pvr6:24.4": "Samukkaṭṭhapadānaṃ natthi tattha āpattīti. #22",
  "pli-tv-pvr6:25.1": "Khandhakapucchāvāro niṭṭhito paṭhamo.",
  "pli-tv-pvr6:26.1": "Tassuddānaṃ",
  "pli-tv-pvr6:27.1": "Upasampadūposatho,",
  "pli-tv-pvr6:27.2": "Vassūpanāyikapavāraṇā;",
  "pli-tv-pvr6:27.3": "Cammabhesajjakathinā,",
  "pli-tv-pvr6:27.4": "Cīvaraṃ campeyyakena ca.",
  "pli-tv-pvr6:28.1": "Kosambakkhandhakaṃ kammaṃ,",
  "pli-tv-pvr6:28.2": "Pārivāsisamuccayā;",
  "pli-tv-pvr6:28.3": "Samathakhuddakā senā,",
  "pli-tv-pvr6:28.4": "Saṃghabhedaṃ samācāro;",
  "pli-tv-pvr6:28.5": "Ṭhapanaṃ bhikkhunikkhandhaṃ,",
  "pli-tv-pvr6:28.6": "Pañcasattasatena cāti."
}