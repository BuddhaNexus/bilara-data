{
  "pli-tv-prv19:0.1": "Parivāra",
  "pli-tv-prv19:0.2": "Dutiyagāthāsaṅgaṇika",
  "pli-tv-prv19:0.3": "1. Kāyikādiāpatti",
  "pli-tv-prv19:1.1": "Kati āpattiyo kāyikā,",
  "pli-tv-prv19:1.2": "Kati vācasikā katā;",
  "pli-tv-prv19:1.3": "Chādentassa kati āpattiyo,",
  "pli-tv-prv19:1.4": "Kati saṃsaggapaccayā.",
  "pli-tv-prv19:2.1": "Chāpattiyo kāyikā,",
  "pli-tv-prv19:2.2": "Cha vācasikā katā;",
  "pli-tv-prv19:2.3": "Chādentassa tisso āpattiyo,",
  "pli-tv-prv19:2.4": "Pañca saṃsaggapaccayā.",
  "pli-tv-prv19:3.1": "Aruṇugge kati āpattiyo,",
  "pli-tv-prv19:3.2": "Kati yāvatatiyakā;",
  "pli-tv-prv19:3.3": "Katettha aṭṭha vatthukā,",
  "pli-tv-prv19:3.4": "Katihi sabbasaṅgaho.",
  "pli-tv-prv19:4.1": "Aruṇugge tisso āpattiyo,",
  "pli-tv-prv19:4.2": "Dve yāvatatiyakā;",
  "pli-tv-prv19:4.3": "Ekettha aṭṭha vatthukā,",
  "pli-tv-prv19:4.4": "Ekena sabbasaṅgaho.",
  "pli-tv-prv19:5.1": "Vinayassa kati mūlāni,",
  "pli-tv-prv19:5.2": "Yāni buddhena paññattā;",
  "pli-tv-prv19:5.3": "Vinayagarukā kati vuttā,",
  "pli-tv-prv19:5.4": "Duṭṭhullacchādanā kati.",
  "pli-tv-prv19:6.1": "Vinayassa dve mūlāni,",
  "pli-tv-prv19:6.2": "yāni buddhena paññattā;",
  "pli-tv-prv19:6.3": "Vinayagarukā dve vuttā,",
  "pli-tv-prv19:6.4": "dve duṭṭhullacchādanā.",
  "pli-tv-prv19:7.1": "Gāmantare kati āpattiyo,",
  "pli-tv-prv19:7.2": "Kati nadipārapaccayā;",
  "pli-tv-prv19:7.3": "Katimaṃsesu thullaccayaṃ,",
  "pli-tv-prv19:7.4": "Katimaṃsesu dukkaṭaṃ.",
  "pli-tv-prv19:8.1": "Gāmantare catasso āpattiyo,",
  "pli-tv-prv19:8.2": "Catasso nadipārapaccayā;",
  "pli-tv-prv19:8.3": "Ekamaṃse thullaccayaṃ,",
  "pli-tv-prv19:8.4": "Navamaṃsesu dukkaṭaṃ.",
  "pli-tv-prv19:9.1": "Kati vācasikā rattiṃ,",
  "pli-tv-prv19:9.2": "Kati vācasikā divā;",
  "pli-tv-prv19:9.3": "Dadamānassa kati āpattiyo,",
  "pli-tv-prv19:9.4": "Paṭiggaṇhantassa kittakā.",
  "pli-tv-prv19:10.1": "Dve vācasikā rattiṃ,",
  "pli-tv-prv19:10.2": "Dve vācasikā divā;",
  "pli-tv-prv19:10.3": "Dadamānassa tisso āpattiyo,",
  "pli-tv-prv19:10.4": "Cattāro ca paṭiggahe.",
  "pli-tv-prv19:10.5": "2. Desanāgāminiyādiāpatti",
  "pli-tv-prv19:11.1": "Kati desanāgāminiyo,",
  "pli-tv-prv19:11.2": "Kati sappaṭikammā katā;",
  "pli-tv-prv19:11.3": "Katettha appaṭikammā vuttā,",
  "pli-tv-prv19:11.4": "Buddhenādiccabandhunā.",
  "pli-tv-prv19:12.1": "Pañca desanāgāminiyo,",
  "pli-tv-prv19:12.2": "Cha sappaṭikammā katā;",
  "pli-tv-prv19:12.3": "Ekettha appaṭikammā vuttā,",
  "pli-tv-prv19:12.4": "Buddhenādiccabandhunā.",
  "pli-tv-prv19:13.1": "Vinayagarukā kati vuttā,",
  "pli-tv-prv19:13.2": "Kāyavācasikāni ca;",
  "pli-tv-prv19:13.3": "Kati vikāle dhaññaraso,",
  "pli-tv-prv19:13.4": "Kati ñatticatutthena sammuti.",
  "pli-tv-prv19:14.1": "Vinayagarukā dve vuttā,",
  "pli-tv-prv19:14.2": "Kāyavācasikāni ca;",
  "pli-tv-prv19:14.3": "Eko vikāle dhaññaraso,",
  "pli-tv-prv19:14.4": "Ekā ñatticatutthena sammuti.",
  "pli-tv-prv19:15.1": "Pārājikā kāyikā kati,",
  "pli-tv-prv19:15.2": "kati saṃvāsakabhūmiyo;",
  "pli-tv-prv19:15.3": "Katinaṃ ratticchedo,",
  "pli-tv-prv19:15.4": "paññattā dvaṅgulā kati.",
  "pli-tv-prv19:16.1": "Pārājikā kāyikā dve,",
  "pli-tv-prv19:16.2": "dve saṃvāsakabhūmiyo;",
  "pli-tv-prv19:16.3": "Dvinnaṃ ratticchedo,",
  "pli-tv-prv19:16.4": "paññattā dvaṅgulā duve.",
  "pli-tv-prv19:17.1": "Katattānaṃ vadhitvāna,",
  "pli-tv-prv19:17.2": "katihi saṃgho bhijjati;",
  "pli-tv-prv19:17.3": "Katettha paṭhamāpattikā,",
  "pli-tv-prv19:17.4": "ñattiyā karaṇā kati.",
  "pli-tv-prv19:18.1": "Dve attānaṃ vadhitvāna,",
  "pli-tv-prv19:18.2": "dvīhi saṃgho bhijjati;",
  "pli-tv-prv19:18.3": "Dvettha paṭhamāpattikā,",
  "pli-tv-prv19:18.4": "ñattiyā karaṇā duve.",
  "pli-tv-prv19:19.1": "Pāṇātipāte kati āpattiyo,",
  "pli-tv-prv19:19.2": "Vācā pārājikā kati;",
  "pli-tv-prv19:19.3": "Obhāsanā kati vuttā,",
  "pli-tv-prv19:19.4": "Sañcarittena vā kati.",
  "pli-tv-prv19:20.1": "Pāṇātipāte tisso āpattiyo,",
  "pli-tv-prv19:20.2": "Vācā pārājikā tayo;",
  "pli-tv-prv19:20.3": "Obhāsanā tayo vuttā,",
  "pli-tv-prv19:20.4": "Sañcarittena vā tayo.",
  "pli-tv-prv19:21.1": "Kati puggalā na upasampādetabbā,",
  "pli-tv-prv19:21.2": "Kati kammānaṃ saṅgahā;",
  "pli-tv-prv19:21.3": "Nāsitakā kati vuttā,",
  "pli-tv-prv19:21.4": "Katinaṃ ekavācikā.",
  "pli-tv-prv19:22.1": "Tayo puggalā na upasampādetabbā,",
  "pli-tv-prv19:22.2": "Tayo kammānaṃ saṅgahā;",
  "pli-tv-prv19:22.3": "Nāsitakā tayo vuttā,",
  "pli-tv-prv19:22.4": "Tiṇṇannaṃ ekavācikā.",
  "pli-tv-prv19:23.1": "Adinnādāne kati āpattiyo,",
  "pli-tv-prv19:23.2": "Kati methunapaccayā;",
  "pli-tv-prv19:23.3": "Chindantassa kati āpattiyo,",
  "pli-tv-prv19:23.4": "Kati chaḍḍitapaccayā.",
  "pli-tv-prv19:24.1": "Adinnādāne tisso āpattiyo,",
  "pli-tv-prv19:24.2": "Catasso methunapaccayā;",
  "pli-tv-prv19:24.3": "Chindantassa tisso āpattiyo,",
  "pli-tv-prv19:24.4": "Pañca chaḍḍitapaccayā.",
  "pli-tv-prv19:25.1": "Bhikkhunovādakavaggasmiṃ,",
  "pli-tv-prv19:25.2": "pācittiyena dukkaṭā;",
  "pli-tv-prv19:25.3": "Katettha navakā vuttā,",
  "pli-tv-prv19:25.4": "katinaṃ cīvarena ca.",
  "pli-tv-prv19:26.1": "Bhikkhunovādakavaggasmiṃ,",
  "pli-tv-prv19:26.2": "Pācittiyena dukkaṭā katā;",
  "pli-tv-prv19:26.3": "Caturettha navakā vuttā,",
  "pli-tv-prv19:26.4": "Dvinnaṃ cīvarena ca.",
  "pli-tv-prv19:27.1": "Bhikkhunīnañca akkhātā,",
  "pli-tv-prv19:27.2": "Pāṭidesaniyā kati;",
  "pli-tv-prv19:27.3": "Bhuñjantāmakadhaññena,",
  "pli-tv-prv19:27.4": "Pācittiyena dukkaṭā kati.",
  "pli-tv-prv19:28.1": "Bhikkhunīnañca akkhātā,",
  "pli-tv-prv19:28.2": "Aṭṭha pāṭidesanīyā katā;",
  "pli-tv-prv19:28.3": "Bhuñjantāmakadhaññena,",
  "pli-tv-prv19:28.4": "Pācittiyena dukkaṭā katā.",
  "pli-tv-prv19:29.1": "Gacchantassa kati āpattiyo,",
  "pli-tv-prv19:29.2": "Ṭhitassa cāpi kittakā;",
  "pli-tv-prv19:29.3": "Nisinnassa kati āpattiyo,",
  "pli-tv-prv19:29.4": "Nipannassāpi kittakā.",
  "pli-tv-prv19:30.1": "Gacchantassa catasso āpattiyo,",
  "pli-tv-prv19:30.2": "Ṭhitassa cāpi tattakā;",
  "pli-tv-prv19:30.3": "Nisinnassa catasso āpattiyo,",
  "pli-tv-prv19:30.4": "Nipannassāpi tattakā.",
  "pli-tv-prv19:30.5": "3. Pācittiya",
  "pli-tv-prv19:31.1": "Kati pācittiyāni,",
  "pli-tv-prv19:31.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:31.3": "Apubbaṃ acarimaṃ,",
  "pli-tv-prv19:31.4": "āpajjeyya ekato.",
  "pli-tv-prv19:32.1": "Pañca pācittiyāni,",
  "pli-tv-prv19:32.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:32.3": "Apubbaṃ acarimaṃ,",
  "pli-tv-prv19:32.4": "āpajjeyya ekato.",
  "pli-tv-prv19:33.1": "Kati pācittiyāni,",
  "pli-tv-prv19:33.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:33.3": "Apubbaṃ acarimaṃ,",
  "pli-tv-prv19:33.4": "āpajjeyya ekato.",
  "pli-tv-prv19:34.1": "Nava pācittiyāni,",
  "pli-tv-prv19:34.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:34.3": "Apubbaṃ acarimaṃ,",
  "pli-tv-prv19:34.4": "āpajjeyya ekato.",
  "pli-tv-prv19:35.1": "Kati pācittiyāni,",
  "pli-tv-prv19:35.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:35.3": "Kati vācāya deseyya,",
  "pli-tv-prv19:35.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:36.1": "Pañca pācittiyāni,",
  "pli-tv-prv19:36.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:36.3": "Ekavācāya deseyya,",
  "pli-tv-prv19:36.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:37.1": "Kati pācittiyāni,",
  "pli-tv-prv19:37.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:37.3": "Kati vācāya deseyya,",
  "pli-tv-prv19:37.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:38.1": "Nava pācittiyāni,",
  "pli-tv-prv19:38.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:38.3": "Ekavācāya deseyya,",
  "pli-tv-prv19:38.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:39.1": "Kati pācittiyāni,",
  "pli-tv-prv19:39.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:39.3": "Kiñca kittetvā deseyya,",
  "pli-tv-prv19:39.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:40.1": "Pañca pācittiyāni,",
  "pli-tv-prv19:40.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:40.3": "Vatthuṃ kittetvā deseyya,",
  "pli-tv-prv19:40.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:41.1": "Kati pācittiyāni,",
  "pli-tv-prv19:41.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:41.3": "Kiñca kittetvā deseyya,",
  "pli-tv-prv19:41.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:42.1": "Nava pācittiyāni,",
  "pli-tv-prv19:42.2": "sabbāni nānāvatthukāni;",
  "pli-tv-prv19:42.3": "Vatthuṃ kittetvā deseyya,",
  "pli-tv-prv19:42.4": "vuttā ādiccabandhunā.",
  "pli-tv-prv19:43.1": "Yāvatatiyake kati āpattiyo,",
  "pli-tv-prv19:43.2": "Kati vohārapaccayā;",
  "pli-tv-prv19:43.3": "Khādantassa kati āpattiyo,",
  "pli-tv-prv19:43.4": "Kati bhojanapaccayā.",
  "pli-tv-prv19:44.1": "Yāvatatiyake tisso āpattiyo,",
  "pli-tv-prv19:44.2": "Cha vohārapaccayā;",
  "pli-tv-prv19:44.3": "Khādantassa tisso āpattiyo,",
  "pli-tv-prv19:44.4": "Pañca bhojanapaccayā.",
  "pli-tv-prv19:45.1": "Sabbā yāvatatiyakā,",
  "pli-tv-prv19:45.2": "Kati ṭhānāni gacchanti;",
  "pli-tv-prv19:45.3": "Katinañceva āpatti,",
  "pli-tv-prv19:45.4": "Katinaṃ adhikaraṇena ca.",
  "pli-tv-prv19:46.1": "Sabbā yāvatatiyakā,",
  "pli-tv-prv19:46.2": "Pañca ṭhānāni gacchanti;",
  "pli-tv-prv19:46.3": "Pañcannañceva āpatti,",
  "pli-tv-prv19:46.4": "Pañcannaṃ adhikaraṇena ca.",
  "pli-tv-prv19:47.1": "Katinaṃ vinicchayo hoti,",
  "pli-tv-prv19:47.2": "katinaṃ vūpasamena ca;",
  "pli-tv-prv19:47.3": "Katinañceva anāpatti,",
  "pli-tv-prv19:47.4": "katihi ṭhānehi sobhati.",
  "pli-tv-prv19:48.1": "Pañcannaṃ vinicchayo hoti,",
  "pli-tv-prv19:48.2": "pañcannaṃ vūpasamena ca;",
  "pli-tv-prv19:48.3": "Pañcannañceva anāpatti,",
  "pli-tv-prv19:48.4": "tīhi ṭhānehi sobhati.",
  "pli-tv-prv19:49.1": "Kati kāyikā rattiṃ,",
  "pli-tv-prv19:49.2": "Kati kāyikā divā;",
  "pli-tv-prv19:49.3": "Nijjhāyantassa kati āpatti,",
  "pli-tv-prv19:49.4": "Kati piṇḍapātapaccayā.",
  "pli-tv-prv19:50.1": "Dve kāyikā rattiṃ,",
  "pli-tv-prv19:50.2": "Dve kāyikā divā;",
  "pli-tv-prv19:50.3": "Nijjhāyantassa ekā āpatti,",
  "pli-tv-prv19:50.4": "Ekā piṇḍapātapaccayā.",
  "pli-tv-prv19:51.1": "Katānisaṃse sampassaṃ,",
  "pli-tv-prv19:51.2": "paresaṃ saddhāya desaye;",
  "pli-tv-prv19:51.3": "Ukkhittakā kati vuttā,",
  "pli-tv-prv19:51.4": "kati sammāvattanā.",
  "pli-tv-prv19:52.1": "Aṭṭhānisaṃse sampassaṃ,",
  "pli-tv-prv19:52.2": "Paresaṃ saddhāya desaye;",
  "pli-tv-prv19:52.3": "Ukkhittakā tayo vuttā,",
  "pli-tv-prv19:52.4": "Tecattālīsa sammāvattanā.",
  "pli-tv-prv19:53.1": "Kati ṭhāne musāvādo,",
  "pli-tv-prv19:53.2": "kati paramanti vuccati;",
  "pli-tv-prv19:53.3": "Kati pāṭidesanīyā,",
  "pli-tv-prv19:53.4": "katinaṃ desanāya ca.",
  "pli-tv-prv19:54.1": "Pañca ṭhāne musāvādo,",
  "pli-tv-prv19:54.2": "Cuddasa paramanti vuccati;",
  "pli-tv-prv19:54.3": "Dvādasa pāṭidesanīyā,",
  "pli-tv-prv19:54.4": "Catunnaṃ desanāya ca.",
  "pli-tv-prv19:55.1": "Kataṅgiko musāvādo,",
  "pli-tv-prv19:55.2": "kati uposathaṅgāni;",
  "pli-tv-prv19:55.3": "Kati dūteyyaṅgāni,",
  "pli-tv-prv19:55.4": "kati titthiyavattanā.",
  "pli-tv-prv19:56.1": "Aṭṭhaṅgiko musāvādo,",
  "pli-tv-prv19:56.2": "aṭṭha uposathaṅgāni;",
  "pli-tv-prv19:56.3": "Aṭṭha dūteyyaṅgāni,",
  "pli-tv-prv19:56.4": "aṭṭha titthiyavattanā.",
  "pli-tv-prv19:57.1": "Kativācikā upasampadā,",
  "pli-tv-prv19:57.2": "Katinaṃ paccuṭṭhātabbaṃ;",
  "pli-tv-prv19:57.3": "Katinaṃ āsanaṃ dātabbaṃ,",
  "pli-tv-prv19:57.4": "Bhikkhunovādako katihi.",
  "pli-tv-prv19:58.1": "Aṭṭhavācikā upasampadā,",
  "pli-tv-prv19:58.2": "Aṭṭhannaṃ paccuṭṭhātabbaṃ;",
  "pli-tv-prv19:58.3": "Aṭṭhannaṃ āsanaṃ dātabbaṃ,",
  "pli-tv-prv19:58.4": "Bhikkhunovādako aṭṭhahi.",
  "pli-tv-prv19:59.1": "Katinaṃ chejjaṃ hoti,",
  "pli-tv-prv19:59.2": "katinaṃ thullaccayaṃ;",
  "pli-tv-prv19:59.3": "Katinañceva anāpatti,",
  "pli-tv-prv19:59.4": "sabbesaṃ ekavatthukā.",
  "pli-tv-prv19:60.1": "Ekassa chejjaṃ hoti,",
  "pli-tv-prv19:60.2": "catunnaṃ thullaccayaṃ;",
  "pli-tv-prv19:60.3": "Catunnañceva anāpatti,",
  "pli-tv-prv19:60.4": "sabbesaṃ ekavatthukā.",
  "pli-tv-prv19:61.1": "Kati āghātavatthūni,",
  "pli-tv-prv19:61.2": "katihi saṃgho bhijjati;",
  "pli-tv-prv19:61.3": "Katettha paṭhamāpattikā,",
  "pli-tv-prv19:61.4": "ñattiyā karaṇā kati.",
  "pli-tv-prv19:62.1": "Nava āghātavatthūni,",
  "pli-tv-prv19:62.2": "navahi saṃgho bhijjati;",
  "pli-tv-prv19:62.3": "Navettha paṭhamāpattikā,",
  "pli-tv-prv19:62.4": "ñattiyā karaṇā nava.",
  "pli-tv-prv19:62.5": "4. Avandanīyapuggalādi",
  "pli-tv-prv19:63.1": "Kati puggalā nābhivādetabbā,",
  "pli-tv-prv19:63.2": "Añjalisāmicena ca;",
  "pli-tv-prv19:63.3": "Katinaṃ dukkaṭaṃ hoti,",
  "pli-tv-prv19:63.4": "Kati cīvaradhāraṇā.",
  "pli-tv-prv19:64.1": "Dasa puggalā nābhivādetabbā,",
  "pli-tv-prv19:64.2": "Añjalisāmicena ca;",
  "pli-tv-prv19:64.3": "Dasannaṃ dukkaṭaṃ hoti,",
  "pli-tv-prv19:64.4": "Dasa cīvaradhāraṇā.",
  "pli-tv-prv19:65.1": "Katinaṃ vassaṃvuṭṭhānaṃ,",
  "pli-tv-prv19:65.2": "dātabbaṃ idha cīvaraṃ;",
  "pli-tv-prv19:65.3": "Katinaṃ sante dātabbaṃ,",
  "pli-tv-prv19:65.4": "katinañceva na dātabbaṃ.",
  "pli-tv-prv19:66.1": "Pañcannaṃ vassaṃvuṭṭhānaṃ,",
  "pli-tv-prv19:66.2": "dātabbaṃ idha cīvaraṃ;",
  "pli-tv-prv19:66.3": "Sattannaṃ sante dātabbaṃ,",
  "pli-tv-prv19:66.4": "soḷasannaṃ na dātabbaṃ.",
  "pli-tv-prv19:67.1": "Katisataṃ rattisataṃ,",
  "pli-tv-prv19:67.2": "āpattiyo chādayitvāna;",
  "pli-tv-prv19:67.3": "Kati rattiyo vasitvāna,",
  "pli-tv-prv19:67.4": "mucceyya pārivāsiko.",
  "pli-tv-prv19:68.1": "Dasasataṃ rattisataṃ,",
  "pli-tv-prv19:68.2": "āpattiyo chādayitvāna;",
  "pli-tv-prv19:68.3": "Dasa rattiyo vasitvāna,",
  "pli-tv-prv19:68.4": "mucceyya pārivāsiko.",
  "pli-tv-prv19:69.1": "Kati kammadosā vuttā,",
  "pli-tv-prv19:69.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:69.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:69.4": "sabbeva adhammikā kati.",
  "pli-tv-prv19:70.1": "Dvādasa kammadosā vuttā,",
  "pli-tv-prv19:70.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:70.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:70.4": "sabbeva adhammikā katā.",
  "pli-tv-prv19:71.1": "Kati kammasampattiyo vuttā,",
  "pli-tv-prv19:71.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:71.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:71.4": "Sabbeva dhammikā kati.",
  "pli-tv-prv19:72.1": "Catasso kammasampattiyo vuttā,",
  "pli-tv-prv19:72.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:72.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:72.4": "Sabbeva dhammikā katā.",
  "pli-tv-prv19:73.1": "Kati kammāni vuttāni,",
  "pli-tv-prv19:73.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:73.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:73.4": "dhammikā adhammikā kati.",
  "pli-tv-prv19:74.1": "Cha kammāni vuttāni,",
  "pli-tv-prv19:74.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:74.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:74.4": "ekettha dhammikā katā;",
  "pli-tv-prv19:74.5": "Pañca adhammikā vuttā,",
  "pli-tv-prv19:74.6": "buddhenādiccabandhunā.",
  "pli-tv-prv19:75.1": "Kati kammāni vuttāni,",
  "pli-tv-prv19:75.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:75.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:75.4": "dhammikā adhammikā kati.",
  "pli-tv-prv19:76.1": "Cattāri kammāni vuttāni,",
  "pli-tv-prv19:76.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:76.3": "Campāyaṃ vinayavatthusmiṃ,",
  "pli-tv-prv19:76.4": "ekettha dhammikā katā;",
  "pli-tv-prv19:76.5": "Tayo adhammikā vuttā,",
  "pli-tv-prv19:76.6": "buddhenādiccabandhunā.",
  "pli-tv-prv19:77.1": "Yaṃ desitaṃnantajinena tādinā,",
  "pli-tv-prv19:77.2": "Āpattikkhandhāni vivekadassinā;",
  "pli-tv-prv19:77.3": "Katettha sammanti vinā samathehi,",
  "pli-tv-prv19:77.4": "Pucchāmi taṃ brūhi vibhaṅgakovida.",
  "pli-tv-prv19:78.1": "Yaṃ desitaṃnantajinena tādinā,",
  "pli-tv-prv19:78.2": "Āpattikkhandhāni vivekadassinā;",
  "pli-tv-prv19:78.3": "Ekettha sammati vinā samathehi,",
  "pli-tv-prv19:78.4": "Etaṃ te akkhāmi vibhaṅgakovida.",
  "pli-tv-prv19:79.1": "Kati āpāyikā vuttā,",
  "pli-tv-prv19:79.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:79.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:79.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:80.1": "Chaūnadiyaḍḍhasatā vuttā,",
  "pli-tv-prv19:80.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:80.3": "Āpāyikā nerayikā,",
  "pli-tv-prv19:80.4": "Kappaṭṭhā saṃghabhedakā;",
  "pli-tv-prv19:80.5": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:80.6": "Vinayāni suṇohi me.",
  "pli-tv-prv19:81.1": "Kati nāpāyikā vuttā,",
  "pli-tv-prv19:81.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:81.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:81.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:82.1": "Aṭṭhārasa nāpāyikā vuttā,",
  "pli-tv-prv19:82.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:82.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:82.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:83.1": "Kati aṭṭhakā vuttā,",
  "pli-tv-prv19:83.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:83.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:83.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:84.1": "Aṭṭhārasa aṭṭhakā vuttā,",
  "pli-tv-prv19:84.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:84.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:84.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:84.5": "5. Soḷasakammādi",
  "pli-tv-prv19:85.1": "Kati kammāni vuttāni,",
  "pli-tv-prv19:85.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:85.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:85.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:86.1": "Soḷasa kammāni vuttāni,",
  "pli-tv-prv19:86.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:86.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:86.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:87.1": "Kati kammadosā vuttā,",
  "pli-tv-prv19:87.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:87.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:87.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:88.1": "Dvādasa kammadosā vuttā,",
  "pli-tv-prv19:88.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:88.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:88.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:89.1": "Kati kammasampattiyo vuttā,",
  "pli-tv-prv19:89.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:89.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:89.4": "Vinayāni suṇoma te.",
  "pli-tv-prv19:90.1": "Catasso kammasampattiyo vuttā,",
  "pli-tv-prv19:90.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:90.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:90.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:91.1": "Kati kammāni vuttāni,",
  "pli-tv-prv19:91.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:91.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:91.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:92.1": "Cha kammāni vuttāni,",
  "pli-tv-prv19:92.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:92.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:92.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:93.1": "Kati kammāni vuttāni,",
  "pli-tv-prv19:93.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:93.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:93.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:94.1": "Cattāri kammāni vuttāni,",
  "pli-tv-prv19:94.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:94.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:94.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:95.1": "Kati pārājikā vuttā,",
  "pli-tv-prv19:95.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:95.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:95.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:96.1": "Aṭṭha pārājikā vuttā,",
  "pli-tv-prv19:96.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:96.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:96.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:97.1": "Kati saṃghādisesā vuttā,",
  "pli-tv-prv19:97.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:97.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:97.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:98.1": "Tevīsa saṃghādisesā vuttā,",
  "pli-tv-prv19:98.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:98.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:98.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:99.1": "Kati aniyatā vuttā,",
  "pli-tv-prv19:99.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:99.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:99.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:100.1": "Dve aniyatā vuttā,",
  "pli-tv-prv19:100.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:100.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:100.4": "vinayāni suṇohi me.",
  "pli-tv-prv19:101.1": "Kati nissaggiyā vuttā,",
  "pli-tv-prv19:101.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:101.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:101.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:102.1": "Dvecattālīsa nissaggiyā vuttā,",
  "pli-tv-prv19:102.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:102.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:102.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:103.1": "Kati pācittiyā vuttā,",
  "pli-tv-prv19:103.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:103.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:103.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:104.1": "Aṭṭhāsītisataṃ pācittiyā vuttā,",
  "pli-tv-prv19:104.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:104.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:104.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:105.1": "Kati pāṭidesanīyā vuttā,",
  "pli-tv-prv19:105.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:105.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:105.4": "Vinayāni suṇoma te.",
  "pli-tv-prv19:106.1": "Dvādasa pāṭidesanīyā vuttā,",
  "pli-tv-prv19:106.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:106.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:106.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:107.1": "Kati sekhiyā vuttā,",
  "pli-tv-prv19:107.2": "buddhenādiccabandhunā;",
  "pli-tv-prv19:107.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:107.4": "vinayāni suṇoma te.",
  "pli-tv-prv19:108.1": "Pañcasattati sekhiyā vuttā,",
  "pli-tv-prv19:108.2": "Buddhenādiccabandhunā;",
  "pli-tv-prv19:108.3": "Vinayaṃ paṭijānantassa,",
  "pli-tv-prv19:108.4": "Vinayāni suṇohi me.",
  "pli-tv-prv19:109.1": "Yāva supucchitaṃ tayā,",
  "pli-tv-prv19:109.2": "yāva suvissajjitaṃ mayā;",
  "pli-tv-prv19:109.3": "Pucchāvissajjanāya vā,",
  "pli-tv-prv19:109.4": "natthi kiñci asuttakanti.",
  "pli-tv-prv19:110.1": "Dutiyagāthāsaṅgaṇikaṃ niṭṭhitaṃ."
}