{
  "pli-tv-prv2.9:0.1": "Parivāra",
  "pli-tv-prv2.9:0.2": "Bhikkhunivibhaṅga",
  "pli-tv-prv2.9:0.3": "Dutiyabhāga",
  "pli-tv-prv2.9:0.4": "Katthapaññattivāra",
  "pli-tv-prv2.9:0.5": "1. Pārājikakaṇḍa",
  "pli-tv-prv2.9:1.1": "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattaṃ?",
  "pli-tv-prv2.9:1.2": "Kaṃ ārabbha?",
  "pli-tv-prv2.9:1.3": "Kismiṃ vatthusmiṃ …pe…",
  "pli-tv-prv2.9:1.4": "kenābhatanti?",
  "pli-tv-prv2.9:2.1": "Yaṃ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṃsaggaṃ sādiyanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-prv2.9:2.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-prv2.9:2.3": "Kaṃ ārabbhāti?",
  "pli-tv-prv2.9:2.4": "Sundarīnandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-prv2.9:2.5": "Kismiṃ vatthusminti?",
  "pli-tv-prv2.9:2.6": "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyi, tasmiṃ vatthusmiṃ.",
  "pli-tv-prv2.9:2.7": "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?",
  "pli-tv-prv2.9:2.8": "Ekā paññatti.",
  "pli-tv-prv2.9:2.9": "Anupaññatti anuppannapaññatti tasmiṃ natthi.",
  "pli-tv-prv2.9:2.10": "Sabbatthapaññatti, padesapaññattīti?",
  "pli-tv-prv2.9:2.11": "Sabbatthapaññatti.",
  "pli-tv-prv2.9:2.12": "Sādhāraṇapaññatti, asādhāraṇapaññattīti?",
  "pli-tv-prv2.9:2.13": "Asādhāraṇapaññatti.",
  "pli-tv-prv2.9:2.14": "Ekatopaññatti, ubhatopaññattīti?",
  "pli-tv-prv2.9:2.15": "Ekatopaññatti.",
  "pli-tv-prv2.9:2.16": "Catunnaṃ pātimokkhuddesānaṃ katthogadhaṃ kattha pariyāpannanti?",
  "pli-tv-prv2.9:2.17": "Nidānogadhaṃ nidānapariyāpannaṃ?",
  "pli-tv-prv2.9:2.18": "Katamena uddesena uddesaṃ āgacchatīti?",
  "pli-tv-prv2.9:2.19": "Dutiyena uddesena uddesaṃ āgacchati.",
  "pli-tv-prv2.9:2.20": "Catunnaṃ vipattīnaṃ katamā vipattīti?",
  "pli-tv-prv2.9:2.21": "Sīlavipatti.",
  "pli-tv-prv2.9:2.22": "Sattannaṃ āpattikkhandhānaṃ katamo āpattikkhandhoti?",
  "pli-tv-prv2.9:2.23": "Pārājikāpattikkhandho.",
  "pli-tv-prv2.9:2.24": "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhātīti?",
  "pli-tv-prv2.9:2.25": "Ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-prv2.9:2.26": "kāyato ca cittato ca samuṭṭhāti, na vācato …pe…",
  "pli-tv-prv2.9:2.27": "kenābhatanti?",
  "pli-tv-prv2.9:2.28": "Paramparābhataṃ—",
  "pli-tv-prv2.9:3.1": "Upāli dāsako ceva,",
  "pli-tv-prv2.9:3.2": "soṇako siggavo tathā;",
  "pli-tv-prv2.9:3.3": "Moggaliputtena pañcamā,",
  "pli-tv-prv2.9:3.4": "ete jambusirivhaye.",
  "pli-tv-prv2.9:4.1": "…pe…",
  "pli-tv-prv2.9:5.1": "Ete nāgā mahāpaññā,",
  "pli-tv-prv2.9:5.2": "Vinayaññū maggakovidā;",
  "pli-tv-prv2.9:5.3": "Vinayaṃ dīpe pakāsesuṃ,",
  "pli-tv-prv2.9:5.4": "Piṭakaṃ tambapaṇṇiyāti. #1",
  "pli-tv-prv2.9:6.1": "Vajjappaṭicchādanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-prv2.9:6.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-prv2.9:6.3": "Kaṃ ārabbhāti?",
  "pli-tv-prv2.9:6.4": "Thullanandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-prv2.9:6.5": "Kismiṃ vatthusminti?",
  "pli-tv-prv2.9:6.6": "Thullanandā bhikkhunī jānaṃ pārājikaṃ dhammaṃ ajjhāpannaṃ bhikkhuniṃ nevattanā paṭicodesi na gaṇassa ārocesi, tasmiṃ vatthusmiṃ.",
  "pli-tv-prv2.9:6.7": "Ekā paññatti.",
  "pli-tv-prv2.9:6.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-prv2.9:6.9": "dhuranikkhepe …pe…. #2",
  "pli-tv-prv2.9:7.1": "Yāvatatiyaṃ samanubhāsanāya na paṭinissajjanapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-prv2.9:7.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-prv2.9:7.3": "Kaṃ ārabbhāti?",
  "pli-tv-prv2.9:7.4": "Thullanandaṃ bhikkhuniṃ ārabbha.",
  "pli-tv-prv2.9:7.5": "Kismiṃ vatthusminti?",
  "pli-tv-prv2.9:7.6": "Thullanandā bhikkhunī samaggena saṃghena ukkhittaṃ ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ anuvatti, tasmiṃ vatthusmiṃ.",
  "pli-tv-prv2.9:7.7": "Ekā paññatti.",
  "pli-tv-prv2.9:7.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-prv2.9:7.9": "dhuranikkhepe …pe…. #3",
  "pli-tv-prv2.9:8.1": "Aṭṭhamaṃ vatthuṃ paripūraṇapaccayā pārājikaṃ kattha paññattanti?",
  "pli-tv-prv2.9:8.2": "Sāvatthiyaṃ paññattaṃ.",
  "pli-tv-prv2.9:8.3": "Kaṃ ārabbhāti?",
  "pli-tv-prv2.9:8.4": "Chabbaggiyā bhikkhuniyo ārabbha.",
  "pli-tv-prv2.9:8.5": "Kismiṃ vatthusminti?",
  "pli-tv-prv2.9:8.6": "Chabbaggiyā bhikkhuniyo aṭṭhamaṃ vatthuṃ paripūresuṃ, tasmiṃ vatthusmiṃ.",
  "pli-tv-prv2.9:8.7": "Ekā paññatti.",
  "pli-tv-prv2.9:8.8": "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-prv2.9:8.9": "dhuranikkhepe …pe…. #4",
  "pli-tv-prv2.9:9.1": "Pārājikā niṭṭhitā."
}