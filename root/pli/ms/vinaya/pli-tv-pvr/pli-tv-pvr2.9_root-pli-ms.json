{
  "pli-tv-pvr2.9:0.1": "Parivāra",
  "pli-tv-pvr2.9:0.2": "Bhikkhunivibhaṅga",
  "pli-tv-pvr2.9:0.3": "Dutiyabhāga",
  "pli-tv-pvr2.9:0.4": "Katthapaññattivāra",
  "pli-tv-pvr2.9:0.5": "1. Pārājikakaṇḍa",
  "pli-tv-pvr2.9:1.1": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṁsaggaṁ sādiyanapaccayā pārājikaṁ kattha paññattaṁ?",
  "pli-tv-pvr2.9:1.2": "Kaṁ ārabbha?",
  "pli-tv-pvr2.9:1.3": "Kismiṁ vatthusmiṁ …pe…",
  "pli-tv-pvr2.9:1.4": "kenābhatanti?",
  "pli-tv-pvr2.9:2.1": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṁsaggaṁ sādiyanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:2.2": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:2.3": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:2.4": "Sundarīnandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:2.5": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:2.6": "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṁsaggaṁ sādiyi, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:2.7": "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?",
  "pli-tv-pvr2.9:2.8": "Ekā paññatti.",
  "pli-tv-pvr2.9:2.9": "Anupaññatti anuppannapaññatti tasmiṁ natthi.",
  "pli-tv-pvr2.9:2.10": "Sabbatthapaññatti, padesapaññattīti?",
  "pli-tv-pvr2.9:2.11": "Sabbatthapaññatti.",
  "pli-tv-pvr2.9:2.12": "Sādhāraṇapaññatti, asādhāraṇapaññattīti?",
  "pli-tv-pvr2.9:2.13": "Asādhāraṇapaññatti.",
  "pli-tv-pvr2.9:2.14": "Ekatopaññatti, ubhatopaññattīti?",
  "pli-tv-pvr2.9:2.15": "Ekatopaññatti.",
  "pli-tv-pvr2.9:2.16": "Catunnaṁ pātimokkhuddesānaṁ katthogadhaṁ kattha pariyāpannanti?",
  "pli-tv-pvr2.9:2.17": "Nidānogadhaṁ nidānapariyāpannaṁ?",
  "pli-tv-pvr2.9:2.18": "Katamena uddesena uddesaṁ āgacchatīti?",
  "pli-tv-pvr2.9:2.19": "Dutiyena uddesena uddesaṁ āgacchati.",
  "pli-tv-pvr2.9:2.20": "Catunnaṁ vipattīnaṁ katamā vipattīti?",
  "pli-tv-pvr2.9:2.21": "Sīlavipatti.",
  "pli-tv-pvr2.9:2.22": "Sattannaṁ āpattikkhandhānaṁ katamo āpattikkhandhoti?",
  "pli-tv-pvr2.9:2.23": "Pārājikāpattikkhandho.",
  "pli-tv-pvr2.9:2.24": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhātīti?",
  "pli-tv-pvr2.9:2.25": "Ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:2.26": "kāyato ca cittato ca samuṭṭhāti, na vācato …pe…",
  "pli-tv-pvr2.9:2.27": "kenābhatanti?",
  "pli-tv-pvr2.9:2.28": "Paramparābhataṁ—",
  "pli-tv-pvr2.9:3.1": "Upāli dāsako ceva,",
  "pli-tv-pvr2.9:3.2": "soṇako siggavo tathā;",
  "pli-tv-pvr2.9:3.3": "Moggaliputtena pañcamā,",
  "pli-tv-pvr2.9:3.4": "ete jambusirivhaye.",
  "pli-tv-pvr2.9:4.1": "…pe…",
  "pli-tv-pvr2.9:5.1": "Ete nāgā mahāpaññā,",
  "pli-tv-pvr2.9:5.2": "Vinayaññū maggakovidā;",
  "pli-tv-pvr2.9:5.3": "Vinayaṁ dīpe pakāsesuṁ,",
  "pli-tv-pvr2.9:5.4": "Piṭakaṁ tambapaṇṇiyāti. #1",
  "pli-tv-pvr2.9:6.1": "Vajjappaṭicchādanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:6.2": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:6.3": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:6.4": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:6.5": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:6.6": "Thullanandā bhikkhunī jānaṁ pārājikaṁ dhammaṁ ajjhāpannaṁ bhikkhuniṁ nevattanā paṭicodesi na gaṇassa ārocesi, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:6.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:6.8": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:6.9": "dhuranikkhepe …pe…. #2",
  "pli-tv-pvr2.9:7.1": "Yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:7.2": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:7.3": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:7.4": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:7.5": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:7.6": "Thullanandā bhikkhunī samaggena saṅghena ukkhittaṁ ariṭṭhaṁ bhikkhuṁ gaddhabādhipubbaṁ anuvatti, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:7.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:7.8": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:7.9": "dhuranikkhepe …pe…. #3",
  "pli-tv-pvr2.9:8.1": "Aṭṭhamaṁ vatthuṁ paripūraṇapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:8.2": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:8.3": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:8.4": "Chabbaggiyā bhikkhuniyo ārabbha.",
  "pli-tv-pvr2.9:8.5": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:8.6": "Chabbaggiyā bhikkhuniyo aṭṭhamaṁ vatthuṁ paripūresuṁ, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:8.7": "Ekā paññatti.",
  "pli-tv-pvr2.9:8.8": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:8.9": "dhuranikkhepe …pe…. #4",
  "pli-tv-pvr2.9:9.1": "Pārājikā niṭṭhitā."
}