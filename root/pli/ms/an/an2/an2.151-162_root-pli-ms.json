{
  "an2.151-162:0.1": "Aṅguttara Nikāya 2 ",
  "an2.151-162:0.2": "14. Santhāravagga ",
  "an2.151-162:0.3": "151 ",
  "an2.151-162:151.1.1": "“Dveme, bhikkhave, santhārā. ",
  "an2.151-162:151.1.2": "Katame dve? ",
  "an2.151-162:151.1.3": "Āmisasanthāro ca dhammasanthāro ca. ",
  "an2.151-162:151.1.4": "Ime kho, bhikkhave, dve santhārā. ",
  "an2.151-162:151.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ santhārānaṃ yadidaṃ dhammasanthāro”ti. ",
  "an2.151-162:151.1.6": "152 ",
  "an2.151-162:152.1.1": "“Dveme, bhikkhave, paṭisanthārā. ",
  "an2.151-162:152.1.2": "Katame dve? ",
  "an2.151-162:152.1.3": "Āmisapaṭisanthāro ca dhammapaṭisanthāro ca. ",
  "an2.151-162:152.1.4": "Ime kho, bhikkhave, dve paṭisanthārā. ",
  "an2.151-162:152.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ paṭisanthārānaṃ yadidaṃ dhammapaṭisanthāro”ti. ",
  "an2.151-162:152.1.6": "153 ",
  "an2.151-162:153.1.1": "“Dvemā, bhikkhave, esanā. ",
  "an2.151-162:153.1.2": "Katamā dve? ",
  "an2.151-162:153.1.3": "Āmisesanā ca dhammesanā ca. ",
  "an2.151-162:153.1.4": "Imā kho, bhikkhave, dve esanā. ",
  "an2.151-162:153.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ esanānaṃ yadidaṃ dhammesanā”ti. ",
  "an2.151-162:153.1.6": "154 ",
  "an2.151-162:154.1.1": "“Dvemā, bhikkhave, pariyesanā. ",
  "an2.151-162:154.1.2": "Katamā dve? ",
  "an2.151-162:154.1.3": "Āmisapariyesanā ca dhammapariyesanā ca. ",
  "an2.151-162:154.1.4": "Imā kho, bhikkhave, dve pariyesanā. ",
  "an2.151-162:154.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyesanānaṃ yadidaṃ dhammapariyesanā”ti. ",
  "an2.151-162:154.1.6": "155 ",
  "an2.151-162:155.1.1": "“Dvemā, bhikkhave, pariyeṭṭhiyo. ",
  "an2.151-162:155.1.2": "Katamā dve? ",
  "an2.151-162:155.1.3": "Āmisapariyeṭṭhi ca dhammapariyeṭṭhi ca. ",
  "an2.151-162:155.1.4": "Imā kho, bhikkhave, dve pariyeṭṭhiyo. ",
  "an2.151-162:155.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyeṭṭhīnaṃ yadidaṃ dhammapariyeṭṭhī”ti. ",
  "an2.151-162:155.1.6": "156 ",
  "an2.151-162:156.1.1": "“Dvemā, bhikkhave, pūjā. ",
  "an2.151-162:156.1.2": "Katamā dve? ",
  "an2.151-162:156.1.3": "Āmisapūjā ca dhammapūjā ca. ",
  "an2.151-162:156.1.4": "Imā kho, bhikkhave, dve pūjā. ",
  "an2.151-162:156.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pūjānaṃ yadidaṃ dhammapūjā”ti. ",
  "an2.151-162:156.1.6": "157 ",
  "an2.151-162:157.1.1": "“Dvemāni, bhikkhave, ātitheyyāni. ",
  "an2.151-162:157.1.2": "Katamāni dve? ",
  "an2.151-162:157.1.3": "Āmisātitheyyañca dhammātitheyyañca. ",
  "an2.151-162:157.1.4": "Imāni kho, bhikkhave, dve ātitheyyāni. ",
  "an2.151-162:157.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ātitheyyānaṃ yadidaṃ dhammātitheyyan”ti. ",
  "an2.151-162:157.1.6": "158 ",
  "an2.151-162:158.1.1": "“Dvemā, bhikkhave, iddhiyo. ",
  "an2.151-162:158.1.2": "Katamā dve? ",
  "an2.151-162:158.1.3": "Āmisiddhi ca dhammiddhi ca. ",
  "an2.151-162:158.1.4": "Imā kho, bhikkhave, dve iddhiyo. ",
  "an2.151-162:158.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ iddhīnaṃ yadidaṃ dhammiddhī”ti. ",
  "an2.151-162:158.1.6": "159 ",
  "an2.151-162:159.1.1": "“Dvemā, bhikkhave, vuddhiyo. ",
  "an2.151-162:159.1.2": "Katamā dve? ",
  "an2.151-162:159.1.3": "Āmisavuddhi ca dhammavuddhi ca. ",
  "an2.151-162:159.1.4": "Imā kho, bhikkhave, dve vuddhiyo. ",
  "an2.151-162:159.1.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ vuddhīnaṃ yadidaṃ dhammavuddhī”ti. ",
  "an2.151-162:159.1.6": "160 ",
  "an2.151-162:160.1.1": "“Dvemāni, bhikkhave, ratanāni. ",
  "an2.151-162:160.1.2": "Katamāni dve? ",
  "an2.151-162:160.1.3": "Āmisaratanañca dhammaratanañca. ",
  "an2.151-162:160.1.4": "Imāni kho, bhikkhave, dve ratanāni. ",
  "an2.151-162:160.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ratanānaṃ yadidaṃ dhammaratanan”ti. ",
  "an2.151-162:160.1.6": "161 ",
  "an2.151-162:161.1.1": "“Dveme, bhikkhave, sannicayā. ",
  "an2.151-162:161.1.2": "Katame dve? ",
  "an2.151-162:161.1.3": "Āmisasannicayo ca dhammasannicayo ca. ",
  "an2.151-162:161.1.4": "Ime kho, bhikkhave, dve sannicayā. ",
  "an2.151-162:161.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sannicayānaṃ yadidaṃ dhammasannicayo”ti. ",
  "an2.151-162:161.1.6": "162 ",
  "an2.151-162:162.1.1": "“Dvemāni, bhikkhave, vepullāni. ",
  "an2.151-162:162.1.2": "Katamāni dve? ",
  "an2.151-162:162.1.3": "Āmisavepullañca dhammavepullañca. ",
  "an2.151-162:162.1.4": "Imāni kho, bhikkhave, dve vepullāni. ",
  "an2.151-162:162.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ vepullānaṃ yadidaṃ dhammavepullan”ti. ",
  "an2.151-162:162.1.6": "Santhāravaggo catuttho. ",
  "~": ""
}