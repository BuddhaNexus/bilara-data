{
  "an2.163-179:0.1": "Aṅguttara Nikāya 2 ",
  "an2.163-179:0.2": "15. Samāpattivagga ",
  "an2.163-179:0.3": "163 ",
  "an2.163-179:163.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:163.1.2": "Katame dve? ",
  "an2.163-179:163.1.3": "Samāpattikusalatā ca samāpattivuṭṭhānakusalatā ca. ",
  "an2.163-179:163.1.4": "Ime kho, bhikkhave, dve dhammā”ti. ",
  "an2.163-179:163.1.5": "164 ",
  "an2.163-179:164.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:164.1.2": "Katame dve? ",
  "an2.163-179:164.1.3": "Ajjavañca maddavañca. ",
  "an2.163-179:164.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:164.1.5": "165 ",
  "an2.163-179:165.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:165.1.2": "Katame dve? ",
  "an2.163-179:165.1.3": "Khanti ca soraccañca. ",
  "an2.163-179:165.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:165.1.5": "166 ",
  "an2.163-179:166.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:166.1.2": "Katame dve? ",
  "an2.163-179:166.1.3": "Sākhalyañca paṭisanthāro ca. ",
  "an2.163-179:166.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:166.1.5": "167 ",
  "an2.163-179:167.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:167.1.2": "Katame dve? ",
  "an2.163-179:167.1.3": "Avihiṃsā ca soceyyañca. ",
  "an2.163-179:167.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:167.1.5": "168 ",
  "an2.163-179:168.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:168.1.2": "Katame dve? ",
  "an2.163-179:168.1.3": "Indriyesu aguttadvāratā ca bhojane amattaññutā ca. ",
  "an2.163-179:168.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:168.1.5": "169 ",
  "an2.163-179:169.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:169.1.2": "Katame dve? ",
  "an2.163-179:169.1.3": "Indriyesu guttadvāratā ca bhojane mattaññutā ca. ",
  "an2.163-179:169.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:169.1.5": "170 ",
  "an2.163-179:170.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:170.1.2": "Katame dve? ",
  "an2.163-179:170.1.3": "Paṭisaṅkhānabalañca bhāvanābalañca. ",
  "an2.163-179:170.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:170.1.5": "171 ",
  "an2.163-179:171.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:171.1.2": "Katame dve? ",
  "an2.163-179:171.1.3": "Satibalañca samādhibalañca. ",
  "an2.163-179:171.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:171.1.5": "172 ",
  "an2.163-179:172.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:172.1.2": "Katame dve? ",
  "an2.163-179:172.1.3": "Samatho ca vipassanā ca. ",
  "an2.163-179:172.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:172.1.5": "173 ",
  "an2.163-179:173.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:173.1.2": "Katame dve? ",
  "an2.163-179:173.1.3": "Sīlavipatti ca diṭṭhivipatti ca. ",
  "an2.163-179:173.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:173.1.5": "174 ",
  "an2.163-179:174.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:174.1.2": "Katame dve? ",
  "an2.163-179:174.1.3": "Sīlasampadā ca diṭṭhisampadā ca. ",
  "an2.163-179:174.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:174.1.5": "175 ",
  "an2.163-179:175.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:175.1.2": "Katame dve? ",
  "an2.163-179:175.1.3": "Sīlavisuddhi ca diṭṭhivisuddhi ca. ",
  "an2.163-179:175.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:175.1.5": "176 ",
  "an2.163-179:176.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:176.1.2": "Katame dve? ",
  "an2.163-179:176.1.3": "Diṭṭhivisuddhi ca yathādiṭṭhissa ca padhānaṃ. ",
  "an2.163-179:176.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:176.1.5": "177 ",
  "an2.163-179:177.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:177.1.2": "Katame dve? ",
  "an2.163-179:177.1.3": "Asantuṭṭhitā ca kusalesu dhammesu, appaṭivānitā ca padhānasmiṃ. ",
  "an2.163-179:177.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:177.1.5": "178 ",
  "an2.163-179:178.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:178.1.2": "Katame dve? ",
  "an2.163-179:178.1.3": "Muṭṭhassaccañca asampajaññañca. ",
  "an2.163-179:178.1.4": "Ime kho, bhikkhave, dve dhammā”. ",
  "an2.163-179:178.1.5": "179 ",
  "an2.163-179:179.1.1": "“Dveme, bhikkhave, dhammā. ",
  "an2.163-179:179.1.2": "Katame dve? ",
  "an2.163-179:179.1.3": "Sati ca sampajaññañca. ",
  "an2.163-179:179.1.4": "Ime kho, bhikkhave, dve dhammā”ti. ",
  "an2.163-179:179.1.5": "Samāpattivaggo pañcamo. ",
  "an2.163-179:179.1.6": "Tatiyo paṇṇāsako samatto. ",
  "~": ""
}