{
  "an2.64-76:0.1": "Aṅguttara Nikāya 2 ",
  "an2.64-76:0.2": "7. Sukhavagga ",
  "an2.64-76:0.3": "64 ",
  "an2.64-76:64.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:64.1.2": "Katamāni dve? ",
  "an2.64-76:64.1.3": "Gihisukhañca pabbajitasukhañca. ",
  "an2.64-76:64.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:64.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ pabbajitasukhan”ti. ",
  "an2.64-76:64.1.6": "65 ",
  "an2.64-76:65.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:65.1.2": "Katamāni dve? ",
  "an2.64-76:65.1.3": "Kāmasukhañca nekkhammasukhañca. ",
  "an2.64-76:65.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:65.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nekkhammasukhan”ti. ",
  "an2.64-76:65.1.6": "66 ",
  "an2.64-76:66.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:66.1.2": "Katamāni dve? ",
  "an2.64-76:66.1.3": "Upadhisukhañca nirupadhisukhañca. ",
  "an2.64-76:66.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:66.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nirupadhisukhan”ti. ",
  "an2.64-76:66.1.6": "67 ",
  "an2.64-76:67.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:67.1.2": "Katamāni dve? ",
  "an2.64-76:67.1.3": "Sāsavasukhañca anāsavasukhañca. ",
  "an2.64-76:67.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:67.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ anāsavasukhan”ti. ",
  "an2.64-76:67.1.6": "68 ",
  "an2.64-76:68.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:68.1.2": "Katamāni dve? ",
  "an2.64-76:68.1.3": "Sāmisañca sukhaṃ nirāmisañca sukhaṃ. ",
  "an2.64-76:68.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:68.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nirāmisaṃ sukhan”ti. ",
  "an2.64-76:68.1.6": "69 ",
  "an2.64-76:69.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:69.1.2": "Katamāni dve? ",
  "an2.64-76:69.1.3": "Ariyasukhañca anariyasukhañca. ",
  "an2.64-76:69.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:69.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ ariyasukhan”ti. ",
  "an2.64-76:69.1.6": "70 ",
  "an2.64-76:70.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:70.1.2": "Katamāni dve? ",
  "an2.64-76:70.1.3": "Kāyikañca sukhaṃ cetasikañca sukhaṃ. ",
  "an2.64-76:70.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:70.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ cetasikaṃ sukhan”ti. ",
  "an2.64-76:70.1.6": "71 ",
  "an2.64-76:71.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:71.1.2": "Katamāni dve? ",
  "an2.64-76:71.1.3": "Sappītikañca sukhaṃ nippītikañca sukhaṃ. ",
  "an2.64-76:71.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:71.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nippītikaṃ sukhan”ti. ",
  "an2.64-76:71.1.6": "72 ",
  "an2.64-76:72.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:72.1.2": "Katamāni dve? ",
  "an2.64-76:72.1.3": "Sātasukhañca upekkhāsukhañca. ",
  "an2.64-76:72.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:72.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ upekkhāsukhan”ti. ",
  "an2.64-76:72.1.6": "73 ",
  "an2.64-76:73.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:73.1.2": "Katamāni dve? ",
  "an2.64-76:73.1.3": "Samādhisukhañca asamādhisukhañca. ",
  "an2.64-76:73.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:73.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ samādhisukhan”ti. ",
  "an2.64-76:73.1.6": "74 ",
  "an2.64-76:74.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:74.1.2": "Katamāni dve? ",
  "an2.64-76:74.1.3": "Sappītikārammaṇañca sukhaṃ nippītikārammaṇañca sukhaṃ. ",
  "an2.64-76:74.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:74.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nippītikārammaṇaṃ sukhan”ti. ",
  "an2.64-76:74.1.6": "75 ",
  "an2.64-76:75.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:75.1.2": "Katamāni dve? ",
  "an2.64-76:75.1.3": "Sātārammaṇañca sukhaṃ upekkhārammaṇañca sukhaṃ. ",
  "an2.64-76:75.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:75.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ upekkhārammaṇaṃ sukhan”ti. ",
  "an2.64-76:75.1.6": "76 ",
  "an2.64-76:76.1.1": "“Dvemāni, bhikkhave, sukhāni. ",
  "an2.64-76:76.1.2": "Katamāni dve? ",
  "an2.64-76:76.1.3": "Rūpārammaṇañca sukhaṃ arūpārammaṇañca sukhaṃ. ",
  "an2.64-76:76.1.4": "Imāni kho, bhikkhave, dve sukhāni. ",
  "an2.64-76:76.1.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ arūpārammaṇaṃ sukhan”ti. ",
  "an2.64-76:76.1.6": "Sukhavaggo dutiyo. ",
  "~": ""
}