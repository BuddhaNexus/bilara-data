{
  "snp5.7:0.1": "Sutta Nipāta 5.7",
  "snp5.7:0.2": "Upasīvamāṇavapucchā",
  "snp5.7:1.1": "“Eko ahaṃ sakka mahantamoghaṃ,",
  "snp5.7:1.2": "(iccāyasmā upasīvo)",
  "snp5.7:1.3": "Anissito no visahāmi tārituṃ;",
  "snp5.7:1.4": "Ārammaṇaṃ brūhi samantacakkhu,",
  "snp5.7:1.5": "Yaṃ nissito oghamimaṃ tareyyaṃ”.",
  "snp5.7:2.1": "“Ākiñcaññaṃ pekkhamāno satimā,",
  "snp5.7:2.2": "(upasīvāti bhagavā)",
  "snp5.7:2.3": "Natthīti nissāya tarassu oghaṃ;",
  "snp5.7:2.4": "Kāme pahāya virato kathāhi,",
  "snp5.7:2.5": "Taṇhakkhayaṃ nattamahābhipassa”.",
  "snp5.7:3.1": "“Sabbesu kāmesu yo vītarāgo,",
  "snp5.7:3.2": "(iccāyasmā upasīvo)",
  "snp5.7:3.3": "Ākiñcaññaṃ nissito hitvā maññaṃ;",
  "snp5.7:3.4": "Saññāvimokkhe parame vimutto,",
  "snp5.7:3.5": "Tiṭṭhe nu so tattha anānuyāyī”.",
  "snp5.7:4.1": "“Sabbesu kāmesu yo vītarāgo,",
  "snp5.7:4.2": "(upasīvāti bhagavā)",
  "snp5.7:4.3": "Ākiñcaññaṃ nissito hitvā maññaṃ;",
  "snp5.7:4.4": "Saññāvimokkhe parame vimutto,",
  "snp5.7:4.5": "Tiṭṭheyya so tattha anānuyāyī”.",
  "snp5.7:5.1": "“Tiṭṭhe ce so tattha anānuyāyī,",
  "snp5.7:5.2": "Pūgampi vassānaṃ samantacakkhu;",
  "snp5.7:5.3": "Tattheva so sītisiyā vimutto,",
  "snp5.7:5.4": "Cavetha viññāṇaṃ tathāvidhassa”.",
  "snp5.7:6.1": "“Accī yathā vātavegena khittā,",
  "snp5.7:6.2": "(upasīvāti bhagavā)",
  "snp5.7:6.3": "Atthaṃ paleti na upeti saṅkhaṃ;",
  "snp5.7:6.4": "Evaṃ munī nāmakāyā vimutto,",
  "snp5.7:6.5": "Atthaṃ paleti na upeti saṅkhaṃ”.",
  "snp5.7:7.1": "“Atthaṅgato so uda vā so natthi,",
  "snp5.7:7.2": "Udāhu ve sassatiyā arogo;",
  "snp5.7:7.3": "Taṃ me munī sādhu viyākarohi,",
  "snp5.7:7.4": "Tathā hi te vidito esa dhammo”.",
  "snp5.7:8.1": "“Atthaṅgatassa na pamāṇamatthi,",
  "snp5.7:8.2": "(upasīvāti bhagavā)",
  "snp5.7:8.3": "Yena naṃ vajjuṃ taṃ tassa natthi;",
  "snp5.7:8.4": "Sabbesu dhammesu samohatesu,",
  "snp5.7:8.5": "Samūhatā vādapathāpi sabbe”ti.",
  "snp5.7:8.6": "Upasīvamāṇavapucchā chaṭṭhī."
}